getgenv().equip = false
getgenv().item = nil
getgenv().num = 0

function equip()
        if getgenv().equip then
local args = {
    [1] = "Pay Virus Requested Amount",
    [2] = getgenv().item,
    [3] = -getgenv().num
}

game:GetService("ReplicatedStorage").Events.StartVote:FireServer(unpack(args))
wait(0.1)
        end
    end

tab.newInput("Input the item name here", "Then input the number on the next input", function(value)
if value == "food" then
getgenv().item = "CannedFood"

elseif value == "water" then
getgenv().item = "Water"

elseif value == "med" then
getgenv().item = "Medkit"

elseif value == "soap" then
getgenv().item = "Soap"

elseif value == "tape" then
getgenv().item = "DuctTape"

elseif value == "flash" then
getgenv().item = "Flashlight"

elseif value == "br" then
getgenv().item = "Battery"

elseif value == "box" then
getgenv().item = "Toolbox"

elseif value == "backpack" then
getgenv().item = "Backpack"

elseif value == "mask" then
getgenv().item = "GasMask"

elseif value == "map" then
getgenv().item = "Map"

elseif value == "bug" then
getgenv().item = "BugSpray"

elseif value == "rat" then
getgenv().item = "RatTrap"

elseif value == "radio" then
getgenv().item = "Radio"

else
    getgenv().item = value
end
end)

tab.newButton("Proceed after you inputted the item name", "Type how much you want or add negatives, Just don't add thousands or it will bug and stop working", function()	
print("yes")
end) 

tab.newInput("Input the amount of item you want here", "Put it after you already put the item name input", function(value)
    getgenv().num = value
wait(0.1)
getgenv().equip = true
equip()
wait(0.01)
getgenv.equip = false
end)

