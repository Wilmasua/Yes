    local coreGui = game:GetService("CoreGui")
local args = {
    [1] = game:GetService("Players").LocalPlayer,
    [2] = true
}

game:GetService("ReplicatedStorage").Events.ActiveVoteCounting:FireServer(unpack(args))
    -- Check if each GUI exists before destroying
    if coreGui:FindFirstChild("SimpleSpy2") then
        coreGui.SimpleSpy2:Destroy()
    end
    
    if coreGui:FindFirstChild("TurtleSpyGUI") then
        coreGui.TurtleSpyGUI:Destroy()
    end
