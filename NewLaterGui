getgenv().namelob = false
getgenv().lobby = nil

function lobequip()
        while getgenv().namelob do 
          
local args = {
    [1] = 0,
    [2] = getgenv().lobby
}

game:GetService("ReplicatedStorage").BuyTheme:InvokeServer(unpack(args))
            wait(0.1)
        end
    end

tab.newInput("Input the name you want the lobby to be here", "After you finish typing it you will get the lobby", function(value)
    getgenv().lobby = value
wait(0.1)
getgenv().namelob = true
lobequip()
wait(0.1)
getgenv.namelob = false
end)

